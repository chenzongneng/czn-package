<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.richstonedt.garnet.modules.job.dao.ScheduleJobLogDao">
	<select id="queryObject" resultType="com.richstonedt.garnet.modules.job.entity.ScheduleJobLogEntity">
		select * from gar_schedule_job_log where log_id = #{value}
	</select>

	<select id="queryList" resultType="com.richstonedt.garnet.modules.job.entity.ScheduleJobLogEntity">
		select * from gar_schedule_job_log 
		<where>
			<if test="jobId != null">
				job_id = #{jobId}
			</if>
		</where>
		order by log_id desc 
		<if test="offset != null and limit != null ">
		limit #{limit} offset #{offset}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(1) from gar_schedule_job_log 
		<where>
			<if test="jobId != null">
			   job_id = #{jobId}
			</if>
		</where>
	</select>

	<insert id="save" parameterType="com.richstonedt.garnet.modules.job.entity.ScheduleJobLogEntity">
		insert into gar_schedule_job_log
		<set>
			<if test="jobId != null">
				job_id = #{jobId},
			</if>
			<if test="beanName != null and beanName != ''">
				bean_name = #{beanName},
			</if>
			<if test="methodName != null and methodName != ''">
				method_name = #{methodName},
			</if>
			<if test="params != null and params != ''">
				params = #{params},
			</if>
			<if test="status != null">
				status = #{status},
			</if>
			<if test="error != null and error != ''">
				error = #{error},
			</if>
			<if test="times != null">
				times = #{times},
			</if>
			<if test="createTime != null">
				create_time = #{createTime}
			</if>
		</set>
	</insert>
	
</mapper>