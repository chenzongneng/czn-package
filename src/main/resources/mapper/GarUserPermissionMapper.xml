<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.richstonedt.garnet.dao.GarUserPermissionDao">

    <select id="getPermissionByUserIdAndAppId" resultType="java.lang.String">
        SELECT up.permission
        FROM gar_v_user_permission up
        WHERE user_id = #{userId} AND app_id = #{appId}
    </select>

</mapper>