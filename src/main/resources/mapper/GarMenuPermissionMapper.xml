<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.richstonedt.garnet.dao.GarMenuPermissionDao">

    <insert id="save">
        INSERT INTO gar_menu_permission
        (
            menu_id,
            permission_id
        )
        VALUES
            (
                #{menuId},
                #{permissionId}
            );
    </insert>

</mapper>